#ifndef _assert
#define _assert(test) do \
if (!(test)) { \
fprintf(stderr, "_assert(%d:%s)@%s:%u[%s]\n", errno, #test, __FILE__, __LINE__, __FUNCTION__); \
assert(test); \
exit(-1); \
} \
while (false)
#endif
